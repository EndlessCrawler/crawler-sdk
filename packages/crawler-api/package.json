		"@types/node": "^20.11.4",
		"jest": "^29.7.0",
		"jest-expect-message": "^1.1.3",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.1",
		"tsc-watch": "^6.0.4",
		"typescript": "^5.3.3"
	},
	"keywords": [
		"dungeon",
		"crawler",
		"generative",
