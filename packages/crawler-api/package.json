{
	"name": "@avante/crawler-api",
	"version": "0.0.1",
	"description": "Endless Crawler Web3 API, Contracts ABI",
	"author": "Studio Avante",
	"license": "CC0-1.0",
	"homepage": "https://github.com/EndlessCrawler",
	"private": false,
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"clean": "rimraf dist tsconfig.build.tsbuildinfo",
		"build": "tsc -p tsconfig.build.json",
		"test": "NODE_OPTIONS=--experimental-vm-modules jest",
